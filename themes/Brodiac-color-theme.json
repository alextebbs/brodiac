  "semanticTokenColors": {
    "interface": "#FFCC00",
    "type": "#FFCC00",
    "namespace": "#FFCC00"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#B3B3B3"
      }
    },
    {
      "scope": [
        "meta.return-type",
        "entity.name.type",
        "entity.name.class",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "bold underline"
      }
    },
    {
      "scope": [
        "meta.return.type",
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars"
      ],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "scope": ["meta.function-call"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": ["punctuation.section.arguments"],
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": ""
      }
    },
    {
      "scope": ["punctuation.section.parameters"],
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable"
      ],
      "settings": {
        "foreground": "#9CDCFE",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.other"
      ],
      "settings": {
        "foreground": "#F3C6BE",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "meta.object-literal.key",
        "meta.object-literal.key entity.name.function"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.package",
        "keyword.import"
      ],
      "settings": {
        "foreground": "#F92672",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.new",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#F92672",
        "fontStyle": ""
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "scope": ["support.class", "support.type", "storage.type"],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "scope": ["string", "string.tag", "string.value"],
      "settings": {
        "foreground": "#F9EC6D"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#D16969"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.other",
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#FC9B1B",
        "fontStyle": ""
      }
    },
    {
      "scope": "support.type.property-name.json",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
