      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#F92672",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": ""
      }
    },
    {
      "scope": ["meta.tag.inline", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#5CDBEF",
        "fontStyle": ""
      }
    },
    {
      "scope": "meta.scope.prerequisites.makefile",
      "settings": {
        "foreground": "#F9EC6D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.separator.key-value.makefile",
      "settings": {
        "foreground": "#F92572"
      }
    },
    {
      "scope": ["keyword.struct.go", "keyword.map.go", "keyword.interface.go"],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": ""
      }
    },
    {
      "scope": ["keyword.type.go", "keyword.function.go", "keyword.var.go"],
      "settings": {
        "foreground": "#F92672",
        "fontStyle": ""
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#4D4D53"
      }
    },
    {
      "scope": "punctuation.definition.comment",
      "settings": {
        "foreground": "#4D4D53"
      }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#FFCC00",
        "fontStyle": ""
      }
    },
    {
      "scope": "variable.scss",
      "settings": {
        "foreground": "#FF7300"
      }
    },
    {
      "scope": "meta.property-list.scss",
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "scope": "variable.interpolation.scss",
      "settings": {
        "foreground": "#6B6B6B"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#9A73E4",
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword.other.important.scss",
      "settings": {
        "foreground": "#FF2B2B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.operator.css",
      "settings": {
        "foreground": "#11C011",
        "fontStyle": ""
      }
    },
    {
      "scope": "punctuation.definition.tag.begin.html",
      "settings": {
        "foreground": "#FF6701",
        "fontStyle": ""
      }
    },
    {
      "scope": "punctuation.definition.tag.end.html",
      "settings": {
        "foreground": "#FF6701",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    },
    {
      "scope": "variable.language.this.ts",
      "settings": {
        "foreground": "#ff92ff"
      }
    },
    {
      "scope": "meta.function-call.ts",
      "settings": {
        "foreground": "#bbff33"
      }
    },
    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#a3e2f7",
        "fontStyle": ""
      }
    },
    {
      "scope": "support.type.primitive",
      "settings": {
        "foreground": "#FFCC00",
        "fontStyle": ""
      }
    },
    {
      "scope": "string.template",
      "settings": {
        "foreground": "#fff8b8",
        "fontStyle": ""
      }
    },
    {
      "scope": "variable.parameter,variable.other.jsdoc",
      "settings": {
        "foreground": "#aee3ff",
        "fontStyle": ""
      }
    },
    {
      "scope": "variable.other.property.ts",
      "settings": {
        "foreground": "#7ad0ff",
        "fontStyle": ""
      }
    }
  ]
}
